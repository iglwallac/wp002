    "name": "web-app",
    "description": " Web Play App",
    "version": "1.200.0",
    "private": true,
    "main": "server.js",
    "license": "UNLICENSED",
    "repository": {
      "type": "git",
      "url": "https://github.com/aglwallac/webplay.gitp"
    },
    "scripts": {
      "build": "mkdir -p tap; NODE_ENV=production ;NODE_PATH=./node_modules/:./lib/:./lib/web-app/ ;npm run init-react-loadable; npm install; gulp; webpack --colors --mode=production; /bin/ls -lFa . ./dist ./dist/server/*",
      "build_and_lint": "mkdir -p tap; NODE_ENV=production ;NODE_PATH=./node_modules/:./lib/:./lib/web-app/ ;npm run lint-tap > tap/lint.tap;npm run init-react-loadable; gulp; webpack --colors --mode=production; /bin/ls -lFa . ./dist ./dist/server/*",
      "build_DEV": "mkdir -p tap; NODE_ENV=development ;NODE_PATH=./node_modules/:./lib/:./lib/web-app/ ;npm run lint-tap > tap/lint.tap;npm run init-react-loadable; gulp; webpack --colors --mode=development; /bin/ls -lFa . ./dist ./dist/server/*",
      "build_good": "npm run init-react-loadable; gulp; webpack --colors --mode=production",
      "build2": "npm run init-react-loadable; gulp",
      "build-webpack": "webpack --colors --mode=production",
      "start": "echo `pwd`; echo '==================================='  ; mkdir /usr/src/app; cp -R ../dist/server/* .; cp -R ../dist/browser/* . ; cp -R . /usr/src/app; cd /usr/src/app; /bin/ls -lFa; node .",
      "startA": "mkdir /usr/src/app; cp -R ./dist/server/* .; cp -R ./dist/browser/* . ; cp . usr/src/app; cd /usr/src/app; /bin/ls -lFa; node .",
      "start_good": "mkdir /usr/src/app; cp -R ./dist/server/* .; cp . usr/src/app; cd /usr/src/app; /bin/ls -lFa; node .",
      "startup": "npm run init-react-loadable; pm2 flush; NODE_PORT=8088 pm2 start process.yml",
      "code-coverage": "NODE_CONFIG_ENV='test' nyc npm run test-tap > /dev/null",
      "code-coverage-report": "nyc report --reporter=text-lcov",
      "dev": "npm run stop && npm start && npm run logs",
      "restart": "node stop.js && node start.js",
      "init-react-loadable": "echo '{}' > react-loadable.json",
      "lint": "eslint --ext .js,.jsx ./",
      "lint-tap": "eslint --format tap --ext .js,.jsx ./",
      "old__lint-tap": "eslint --format tap --quiet --ext .js,.jsx ./",
      "lint-path": "eslint --ext .js,.jsx",
      "lint-git": "git diff --name-only | grep '.\\(js\\|jsx\\)$' | xargs eslint",
      "sec": "nsp check --output summary",
      "test": "mocha --require babel-register lib/test/index.unit.js 'lib/**/*.unit.js'",
      "test-tap": "npm run test -- --reporter tap",
      "test-integration": "node_modules/cucumber/bin/cucumber-js --require-module babel-register --require 'features/**/*.js' --retry 1",
      "test-integration-no-retry": "node_modules/cucumber/bin/cucumber-js --require-module babel-register --require 'features/**/*.js' --retry 0",
      "test-integration-no-retry-windows": "cucumber-js --require-module babel-register --retry 0 features/*.feature lib/**/*.feature",
      "test-integration-parallel": "node_modules/cucumber/bin/cucumber-js --require-module babel-register --require 'features/**/*.js' --retry 2 --parallel 4",
      "post-translations": "gulp i18n-postTranslations",
      "get-translations": "gulp i18n-getTranslations",
      "release": "standard-version",
      "publish-sourcemaps-to-newrelic": "gulp publish-sourcemaps-to-newrelic",
      "postinstall": "rm -rf ./node_modules/.cache"
    },
